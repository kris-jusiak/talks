#include <boost/fusion/include/make_list.hpp>
namespace fusion = boost::fusion;

template<int> struct x {};

int main() {
<% env[:k].times do |k| %>
#if defined(METABENCH)
    /*constexpr*/ auto <%= "xs#{k}" %> = fusion::make_list(
        <%= ((k*n+1)..(k*n+n)).map { |i| "x<#{i}>()" }.join(', ') %>
    );
#endif
<% end %>
}
