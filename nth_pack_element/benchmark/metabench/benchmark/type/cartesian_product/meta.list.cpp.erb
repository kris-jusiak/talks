#include <meta/meta.hpp>

template<int> struct x;
template<int> struct y;

<% env[:k].times do |k| %>

using <%= "xs#{k}" %> = meta::list<<%= ((k*n+1)..(k*n+n)).map { |i| "x<#{i}>" }.join(', ') %>>;
using <%= "ys#{k}" %> = meta::list<<%= ((k*n+1)..(k*n+n)).map { |i| "y<#{i}>" }.join(', ') %>>;

#if defined(METABENCH)
using <%= "result#{k}" %> = meta::cartesian_product<meta::list<<%= "xs#{k}" %>, <%= "ys#{k}" %>>>;
#endif

<% end %>

int main() {}
